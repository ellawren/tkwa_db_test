<div class="page-header">
        <h1>Services</h1>
        <%= link_to "Add Service", new_service_path, :class => "btn primary" %>
</div>
<br /><br />
<h3>Services Listing</h3>
<table>
  <thead>
    <tr>
      <th>id</th>
      <th>Name</th>
      
    </tr>
  </thead>
  <tbody>
    <% @services.each do |service| %>
        <tr>
          <td><%= service.id %></td>
          <td><%= link_to service.name, service %></td>
        </tr>
    <% end %>
    </tbody>
</table>
